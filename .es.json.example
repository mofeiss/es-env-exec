{
  "env": [
    {
      "name": "production",
      "env": {
        "API_BASE_URL": "https://api.production.com",
        "API_TOKEN": "your-production-token-here"
      }
    },
    {
      "name": "staging",
      "env": {
        "API_BASE_URL": "https://api.staging.com",
        "API_TOKEN": "your-staging-token-here"
